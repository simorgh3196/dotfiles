{
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.rulers": [
      80
    ],
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.formatOnType": true,
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "[go]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.insertSpaces": false,
    "editor.snippetSuggestions": "none"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[ruby]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "Shopify.ruby-lsp",
    "editor.formatOnType": true,
    "editor.insertSpaces": true,
    "editor.semanticHighlighting.enabled": true,
    "editor.tabSize": 2
  },
  "[scss]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[swift]": {
    "editor.defaultFormatter": "vknabel.vscode-swiftformat"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.language": "en,jp",
  "cSpell.userWords": [
    "byesig",
    "Cartfile",
    "epub",
    "esbenp",
    "fflate",
    "Gopath",
    "hayakawa",
    "langserver",
    "mozjpeg",
    "neovim",
    "nvim",
    "Podfile",
    "podspec",
    "pubspec",
    "simorgh",
    "solargraph",
    "sonarlint",
    "sourcekit",
    "swiftformat",
    "swiftlint",
    "tabnine",
    "taskeystudio",
    "webtoon"
  ],
  "dart.additionalAnalyzerFileExtensions": [
    "drift"
  ],
  "dart.debugExternalPackageLibraries": false,
  "dart.debugSdkLibraries": false,
  "dart.devToolsBrowser": "default",
  "dart.flutterSdkPaths": [
    "/Users/usr/fvm/versions"
  ],
  "dart.openDevTools": "flutter",
  "dart.previewFlutterUiGuides": true,
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "dart.previewHotReloadOnSaveWatcher": true,
  "dart.sdkPath": null,
  "deno.suggest.completeFunctionCalls": true,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.fontFamily": "Fira Code, Roboto, Menlo",
  "editor.fontLigatures": true,
  "editor.fontSize": 11,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    120
  ],
  "eslint.packageManager": "npm",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "*.dart": "$(capture).g.dart, $(capture).freezed.dart",
    "*.js": "$(capture).js.map, $(capture).min.js, $(capture).d.ts",
    "*.jsx": "$(capture).js",
    "*.ts": "$(capture).js",
    "*.tsx": "$(capture).ts",
    "Cartfile": "Cartfile.resolved",
    "package.json": "package-lock.json, yarn.lock",
    "Package.swift": "Package.resolved",
    "Podfile": "Podfile.lock",
    "pubspec.yaml": ".flutter-plugins, .packages, .dart_tool, .flutter-plugins-dependencies, .metadata, .packages, pubspec.lock, build.yaml, analysis_options.yaml, all_lint_rules.yaml",
    "tsconfig.json": "tsconfig.*.json"
  },
  "files.associations": {
    "Podfile": "ruby",
    "*.podspec.*": "ruby",
    "Package.resolved": "json"
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "gitlens.codeLens.enabled": false,
  "gitProjectManager.baseProjectsFolders": [
    "~/go/src"
  ],
  "go.alternateTools": {
    "go-langserver": "gopls"
  },
  "go.buildOnSave": "off",
  "go.languageServerFlags": [
    "-logfile",
    "-trace"
  ],
  "go.lintTool": "golangci-lint",
  "go.toolsGopath": "~/go",
  "go.toolsManagement.autoUpdate": true,
  "go.useLanguageServer": true,
  "go.vetOnSave": "off",
  "lldb.library": "/Applications/Xcode-14.2.0.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
  "markdown.extension.toc.unorderedList.marker": "*",
  "prettier.jsxSingleQuote": true,
  "prettier.prettierPath": "~/.asdf/shims/prettier",
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "projectManager.git.baseFolders": [
    "~/go/src/"
  ],
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "solargraph.bundlerPath": "~/.asdf/shims/bundler",
  "solargraph.commandPath": "/Users/hayakawa/.asdf/shims/solargraph",
  "solargraph.completion": true,
  "solargraph.diagnostics": true,
  "solargraph.externalServer": {
    "host": "localhost",
    "port": 7658
  },
  "solargraph.formatting": true,
  "solargraph.references": true,
  "solargraph.rename": true,
  "sonarlint.pathToNodeExecutable": "~/.asdf/installs/nodejs/14.21.2/bin/node",
  "sonarlint.rules": {
    "typescript:S2819": {
      "level": "off"
    }
  },
  "sort-json.settings.sortMode": "Objects Only",
  "sourcekit-lsp.serverPath": "/usr/bin/sourcekit-lsp",
  "swift.backgroundCompilation": true,
  "swift.diagnostics": true,
  "swiftformat.options": [
    "--indent",
    "4",
    "--self",
    "init-only",
    "--patternlet",
    "inline",
    "--disable",
    "wrapMultilineStatementBraces",
    "--exclude",
    "Pods,Carthage",
    "--enable",
    "acronyms,blankLineAfterImports,blankLinesBetweenImports,blockComments,docComments,isEmpty,wrapSwitchCases"
  ],
  "swiftformat.path": [
    "/opt/homebrew/bin/swiftformat"
  ],
  "swiftlint.additionalParameters": [],
  "swiftlint.path": [
    "/opt/homebrew/bin/swiftlint"
  ],
  "tabnine.experimentalAutoImports": true,
  "terminal.external.osxExec": "WezTerm.app",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.windowsEnableConpty": false,
  "vscode-neovim.mouseSelectionStartVisualMode": true,
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.darwin": "~/.config/nvim/init.lua",
  "vscode-neovim.neovimWidth": 500,
  "window.zoomLevel": 1,
  "workbench.colorTheme": "Tokyo Night Storm",
  "workbench.iconTheme": "material-icon-theme"
}
